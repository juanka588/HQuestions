<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context="personal.unal.com.healthquestions.GUI.ResultActivityFragment"
    tools:showIn="@layout/activity_result">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="102dp"
        android:layout_height="100dp"
        android:layout_marginLeft="474dp"
        android:layout_marginTop="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@android:color/white"
        app:srcCompat="@drawable/ic_user" />

    <EditText
        android:id="@+id/textView"
        style="@style/OptionStyle"
        android:layout_width="104dp"
        android:layout_height="33dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="14dp"
        android:hint="TextView"
        app:layout_constraintRight_toRightOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:layout_editor_absoluteX="472dp" />

    <TextView
        android:id="@+id/textView2"
        style="@style/OptionStyle"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginRight="323dp"
        android:layout_marginTop="140dp"
        android:text="TextView"
        app:layout_constraintRight_toLeftOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView

        android:id="@+id/textView3"
        style="@style/OptionStyle"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        style="@style/OptionStyle"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />
</android.support.constraint.ConstraintLayout>
